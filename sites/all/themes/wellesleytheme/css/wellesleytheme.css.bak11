/* PROFILES */

.profile-filter-select {
	width: 360px;
	float: left;
	margin-right: 20px;
}

.profile-filter-views {
	width: 100%;
	/*text-align: right;*/
	float: left;
	display: inline-block;
	position: relative;
}

/* Profile Icon View */

.profile-icon-row {
	display: inline-block;
	width: 756px;
	height: auto;
	padding-bottom: 20px;
	clear: both;
}

.profile-icon-row .profile-icon {
	position: relative;
	width: 160px;
	height: 375px;
	margin-right: 10px;
	padding: 10px 8px 12px 8px;
	border: 1px solid #DCDCDC;
	float: left;
	color: #58595B;
    font-family: Georgia,"Times New Roman",Times,serif;
    font-size: 12px;
    line-height: 14px;
    text-align: left;
}

.profile-icon h3 { 
	margin-bottom: 10px;
	margin-top: 10px;
	font: bold 14px/14px Arial, Helvetica, sans-serif;
}

.profile-icon-row .profile-icon.last {
	margin-right: 10px;
}

.profile-icon .profile-icon-desc {
	margin-top: 10px;
}

.profile-icon-desc .headline {
	font: bold 18px/18px 'Times New Roman', Times, Georgia, serif;
	margin-bottom: 0.5em;
}

.profile-icon-desc p {
	margin-bottom: 0.5em;
}

/* Profile List View */

.profile-list-row {
	display: inline-block;
	width: 756px;
	margin-bottom: 12px;
	padding-bottom: 12px;
	border-bottom: 1px solid #DCDCDC;
	clear: both;
}

.profile-list-row.last {
	margin-bottom: 0;
	padding-bottom: 0;
	border-bottom: 0;
}

.profile-list-row .profile-list-image {
	position: relative;
	display: block;
	float: left;
	margin-right: 12px;
}

.profile-list-row .profile-list-desc {
	position: relative;
	display: block;
}

.profile-list-row .headline {
	font: bold 18px/18px 'Times New Roman', Times, Georgia, serif;
	margin-bottom: 2px;
}

.profile-list-row .profile-list-desc p {
	margin-bottom: 0.5em;
}


