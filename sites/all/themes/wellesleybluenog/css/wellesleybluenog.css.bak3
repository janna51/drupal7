/* TABLES */


#content table {
	border-collapse: collapse;
	font-size: 12px;
	text-align: left;
	/*margin: 2em 0;*/
	font-family: Arial, Helvetica, sans-serif; 
}

#content table th, #content table td {
	padding: 7px 10px;
	font-size: 12px;
	border-right: 1px solid #FFF;
	vertical-align: top;
	font-family: Arial, Helvetica, sans-serif; 
}

#content table th {
	background: #000066;
	font-size: 12px;
	color: #FFF;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif; 
}

#content table tr {
	font-size: 12px;
	border-bottom: 1px solid #FFF;
}

#content table tr.odd {
	background: #F3F3F3;
}

#content table tr.even {
	background: #E6E6E6;
}


/* FACTS */

.w-facts-row {
	display: inline-block;
	width: 740px;
	height: auto;
	clear: both;
}

.w-facts-column {
	position: relative;
	width: 233px;
	margin-right: 20px;
	float: left;
}

.w-facts-column.last {
	margin-right: 0;
}

.w-facts-column .w-fact {
	padding: 10px 8px 12px 8px;
	border: 1px solid #DCDCDC;
	margin-bottom: 20px;
}

/*number-h changed to numberLarge and numberSuper*/
.w-fact .numberLarge, 
.w-fact .numberXLarge, 
.w-fact .numberSuper,
.numberSuper,
.numberLarge, 
.numberXLarge {
	font-weight: bold;
	color: #000066;
	font-family: Arial, Helvetica, sans-serif;
	display: block;
	margin-bottom: 10px;
	letter-spacing: -1px;
}

.numberLarge {
	font-size: 36px;
	line-height: 32px;	
}

/*erase after removed from content*/
.numberSuper {
	font-size: 72px;
	line-height: 70px;
}

.numberXLarge {
	font-size: 72px;
	line-height: 70px;
}


/* INLINE IMAGES */

img.top {display: block; margin: 0 auto 30px auto; }
img.alignright { float: right; margin: 0 0 20px 20px; }
img.alignleft { float: left; margin: 0 20px 20px 0; }
a img.alignright { float: right; margin: 0 0 20px 20px; }
a img.alignleft { float: left; margin: 0 20px 20px 0; }

div.alignleft-column { float: left; margin: 0 20px 20px 0; }



/* IMAGES */
.column-wide .imgLeft { 
	float: left; 
	margin: 3px 10px 10px 0; 
}
.column-wide .imgRight { 
	float: right; 
	margin: 3px 0 10px 10px; 
}



/* FOOTER */

#footer {
	position: relative;
	width: 740px;
	height: auto;
	margin: 60px 0 30px 20px;
	padding-top: 20px;
	font-size: 11px;
	line-height: 16px;
	text-align: center;
	color: #808080;
	border-top: 1px solid #DCDCDC;
	clear: both;
}

#footer a {
	color: #808080;
	margin-right: 10px;
}

#footer a:hover {
	color: #808080;
	text-decoration: underline;
}

/* ADMISSION GRID */

#admission-grid {
	position: relative;
	width: 738px;
	height: 350px;
	display: block;
	overflow: hidden;
	margin-bottom: 30px;
}

.admission-grid-slide {
	position: absolute;
	width: 246px;
	height: 175px;
	display: block;
	overflow: hidden;
}

.slide-label {
	position: absolute;
	display: block;
	bottom: 0;
	left: 0;
	height: 32px;
	width: 246px;
	z-index: 100;
}

.slide-label-smoke {
	position: relative;
	display: block;
	height: 32px;
	width: 246px;
	opacity: 0.75;
	background: #000000;
	filter: alpha(opacity=75);
}

.slide-label-text {
	position: absolute;
	top: 0;
	left: 0;
	padding: 6px 9px;
	color: #FFFFFF;
	font: bold 18px/18px 'Times New Roman', Times, Georgia, serif;
}

