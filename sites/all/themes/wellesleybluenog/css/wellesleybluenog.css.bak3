/* BOOKMARK MENU */

.folderlinkstrue {
color:#666666;
font-size:0.9em;
font-weight:bold  !important;
}

#bookmark-menu {
	border-top: 1px dotted #FFFFFF;
	margin-top: 10px;
}

#bookmark-menu ul {
	margin: 0;
	padding: 0;
	list-style: none;
	line-height: 0;
}

/* First Level */

#bookmark-menu li a {
	display: block;
	font: 14px/16px 'Times New Roman', Times, Georgia, serif;
	padding: 8px 10px 7px 12px;
	color: #FFFFFF;
	border-bottom: 1px dotted #FFFFFF;
}

#bookmark-menu li a:hover, #bookmark-menu li a.folderlinkstrue {
	background: url(../images/menu-highlight.gif);
	background-repeat: no-repeat;
	background-position: center;
}

/* Second Level */

#bookmark-menu li li a {
	display: block;
	font: 12px/16px Arial, Helvetica, sans-serif;
	padding: 8px 10px 7px 12px;
	color: #000066;
	background: #E6E6E6 !important;
	border-bottom: 1px dotted #808080;
}

#bookmark-menu li li a:hover {
	background: #DCDCDC !important;
}

#bookmark-menu li li.current a {
	font-weight: bold;
}

#bookmark-menu li li.current li a {
	font-weight: normal;
}

/* Third Level */

#bookmark-menu li li li a {
	display: block;
	font: 12px/16px Arial, Helvetica, sans-serif;
	padding: 8px 10px 7px 24px;
	color: #000066;
	background: #E6E6E6 !important;
	border-bottom: 1px dotted #808080;
}

#bookmark-menu li li li a:hover {
	background: #DCDCDC !important;
}

#bookmark-menu li li li.current a {
	font-weight: bold;
}

#bookmark-menu li li li.current li a {
	font-weight: normal;
}

/* Fourth Level */

#bookmark-menu li li li li a {
	display: block;
	font: 12px/16px Arial, Helvetica, sans-serif;
	padding: 8px 10px 7px 36px;
	color: #000066;
	background: #E6E6E6 !important;
	border-bottom: 1px dotted #808080;
}

#bookmark-menu li li li li a:hover {
	background: #DCDCDC !important;
}

#bookmark-menu li li li li.current a {
	font-weight: bold;
}

#bookmark-menu li li li li.current li a {
	font-weight: normal;
}

/* Fifth Level */

#bookmark-menu li li li li li a {
	display: block;
	font: 12px/16px Arial, Helvetica, sans-serif;
	padding: 8px 10px 7px 48px;
	color: #000066;
	background: #E6E6E6 !important;
	border-bottom: 1px dotted #808080;
}

#bookmark-menu li li li li li a:hover {
	background: #DCDCDC !important;
}

#bookmark-menu li li li li li.current a {
	font-weight: bold;
}

#bookmark-menu li li li li li.current li a {
	font-weight: normal;
}

/* Department Header */

.dept-header {
	font: bold 18px/18px Arial, Helvetica, sans-serif;
	color: #FFF;
	text-align: center;
	padding: 20px 0 30px 0;
}



/* TOOL NAVIGATION */

#tool-nav-wrap {
	position: relative;
	display: block;
	overflow: hidden;
	width: 760px;
	text-align: right;
	height: 34px;
	font-size: 11px;
	padding-top: 12px;	
}


#tool-nav {
	list-style: none;
	margin: 0;
	padding: 0;
	float: right;
	width: auto;
}

#tool-nav li {
	display: -moz-inline-stack;
	display: inline-block;
	zoom: 1; /* IE hack to trigger hasLayout */
	*display: inline; /* IE hack to achieve inline-block behavior */
	margin-right: 10px;
}

#tool-nav li a {
	color: #808080;
}

#tool-nav li a:hover {
	color: #808080;
	text-decoration: underline;
}



/* TOOL NAVIGATION SEARCH */

#tool-nav li.nav-search-wrap {
	margin-right: 0;
}

#tool-nav li.nav-search-wrap .nav-search-field {
	width: 138px;/*was 150px*/
	font-family: Arial, Helvetica, sans-serif;
	color: #808080;
	border: 1px solid #DCDCDC;
	padding: 5px;
}

#tool-nav li.nav-search-wrap .nav-search-button {
	width: 30px;
	font-family: Arial, Helvetica, sans-serif;
	color: #FFFFFF;
	background: #000066;
	border: 1px solid #000066;
	padding: 5px;
	cursor: pointer;
}



/* TOP MENU */


#top-menu-wrap {
	position: relative;
	display: block;
	overflow: hidden;
	width: 770px;/*bluenog edit, was 760px*/
	height: 32px;
}

#top-menu {
	list-style: none;
	margin: 0;
	padding: 0;
	/*font-size: 12px;*/
}

#top-menu li {
	display: block;
	float: left;
}


#top-menu li a {
	position: relative;
	display: block;
	float: left;
	color: #FFFFFF;
	text-align: center;
	padding-top: 8px;
	height: 32px;
	background: none repeat scroll 0 0 #007A87;
	/*next 3 lines, adapted for bluenog
	margin-right:1px;
	padding-left: 9px;
	padding-right: 8px;*/
}

#top-menu li a:hover, #top-menu li a.currentmenu {
	color: #505050;
	background: #E6E6E6;
	/*margin-right:1px; bluenog addition*/
}

/* Set tab widths  */
#top-menu li a.top-menu-about { width: 106px; margin-right: 1px; }
#top-menu li a.top-menu-admission { width: 160px; margin-right: 1px; }
#top-menu li a.top-menu-academics { width: 77px; margin-right: 1px; }
#top-menu li a.top-menu-studentlife { width: 83px; margin-right: 1px; }
#top-menu li a.top-menu-athletics { width: 63px; margin-right: 1px; }
#top-menu li a.top-menu-news { width: 47px; margin-right: 1px; }
#top-menu li a.top-menu-events { width: 55px; margin-right: 1px; }
#top-menu li a.top-menu-administration { width: 96px; margin-right: 1px; }
#top-menu li a.top-menu-alumnae { width: 65px; margin-right: 0; }


/* MODULES */

/* Link Module */

.module-links {
	margin-top: 30px;
	padding: 0 12px;
}

.module-links h5 {
	color: #C8C8C8;
	font-size: 10px;
	letter-spacing: 1px;
	font-weight: normal;
	padding: 0;
	margin: 0;
}

.module-links ul {
	list-style: none;
	margin: 0;
	padding: 0;
}

.module-links li {
	margin-top: 6px;
}

.module-links a {
	color: #FFFFFF;
}

.module-links a:hover {
	text-decoration: underline;
}

/* Address Module */

.module-addr {
	color: #C8C8C8;
	margin-top: 40px;
	margin-bottom: 40px;
	padding: 0 12px;
}

