/* RESET */

p,h1,h2,h3,h4,h5,h6 { margin-top: 0; padding-top: 0; }
img { border: 0; }
form,input { margin: 0; padding: 0; }
a.a-img { border: 0; text-decoration: none; }
div.clear { clear: both; }
select { padding: 0; margin: 0; }

.column-wide ul.contextual-links li {
    line-height: 12px;
    list-style: none;
    margin-bottom: 0.1em;
    margin-left: 0;
    padding: 0;
}

ul li.expanded {
    list-style-image: none;
    list-style-type: none;
}

ul li.leaf {
    list-style-image: none;
    list-style-type: none;
}

ul.menu li {
    margin: 0;
}
li.expanded, li.collapsed, li.leaf {
    margin: 0;
    padding: 0;
}

/* HTML */

html,body {
	margin: 0;
	padding: 0;
	height: 100%;
	width: 100%;
}

td{padding:5px;}

/* BODY */

body {
	color: #505050;
	font: 12px/16px Arial, Helvetica, sans-serif;
}

a[name] { 
	text-decoration: none;
	font-size:inherit;
}

a[name]:hover { 
	text-decoration: none;
	font-size:inherit;
}

a { text-decoration: none; }


h1 { font: bold 36px/40px 'Times New Roman', Times, Georgia, serif; }
h2 { font: bold 24px/28px 'Times New Roman', Times, Georgia, serif; }
h3 { font: bold 18px/18px 'Times New Roman', Times, Georgia, serif; }
h4 { font: bold 16px/16px 'Times New Roman', Times, Georgia, serif; }
#content h5 { font: bold 14px/14px 'Times New Roman', Times, Georgia, serif; }
.PContent h3 { margin-bottom: 10px;}


h1, h2, h3, h4, h5, h6 {
    color: #007A87;
}


hr {
	background: #DCDCDC;
	border: 0;
	clear: both;
	height: 1px;
	margin: 10px 0;
}

/* GENERAL LAYOUT */

#page-bar {
	position: absolute;
	top: 46px;
	left: 0;
	width: 5%;/*25%, original 20%*/
	height: 32px;
	background: none repeat scroll 0 0 #007A87;
}

#banner-image-bar {
	position: absolute;
	top: 46px;
	left: 0;
	width: 5%; /*20%;*/
	height: 335px;
}

#page {
	background: url(../images/bg-global.jpg) repeat-y top left;
	position: relative;
	width: 940px;
	height: auto;
	margin: 0 0 0 3%; /*0 auto;*/
}

#bookmark {
	position: relative;
	width: 178px;
	height: auto;
	background: #000066;
	border-left: 1px solid #FFF;
	border-right: 1px solid #FFF;
	float: left;
	/*z-index: 1000;*/
}


#content-wrapper {
	position: relative;
	width: 79%;
	height: auto;
	float: left;
}
#content {
	position: relative;
	display: inline-block;
	width: 740px;
	height: auto;
	margin: 20px 0 0 20px;
	clear: both;
}

/* Global links */
#content a[href]{ color: #303099; } 
#content a[href]:hover { text-decoration: underline; } 

.column-wide {
	position: relative;
	width: 500px;/*bluenog change: *was 500px, then 740; latest 745*/
	height: auto;
	float: left;
	margin-right: 0px;/*was 20px*/
	margin-left: -5px; /*was 0px*/
}

.column-sidebar {
	position: relative;
	width: 220px;
	height: auto;
	float: right;
}



