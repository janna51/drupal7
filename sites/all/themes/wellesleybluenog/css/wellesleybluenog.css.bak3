/* FILTERS*/

.filters {
	display: inline-block;
	padding: 10px 0;
	margin: 20px 0 40px 0;
	border-top: 1px solid #DCDCDC;
	border-bottom: 1px solid #DCDCDC;
	clear: both;
	width: 730px;
}



/* PROFILES */

.profile-filter-select {
	width: 360px;
	float: left;
	margin-right: 10px;
}

.profile-filter-views {
	width: 360px;
	text-align: right;
	float: right;
}

/* Profile Icon View */

.profile-icon-row {
	display: inline-block;
	width: 730px;
	height: auto;
	padding-bottom: 20px;
	clear: both;
}

.profile-icon-row .profile-icon {
	position: relative;
	width: 215px;
	height: 410px; /*was 380*/
	margin-right: 15px;
	padding: 10px 8px 12px 8px;
	border: 1px solid #DCDCDC;
	float: left;
}

.profile-icon h3 { 
	margin-bottom: 10px;
	font: bold 18px/18px 'Times New Roman', Times, Georgia, serif !important;
	color: #000066 !important;/*addition*/
}

.profile-icon-row .profile-icon.last {
	margin-right: 0;
}

.profile-icon .profile-icon-desc {
	margin-top: 10px;
}

.profile-icon-desc .headline {
	font: bold 18px/18px 'Times New Roman', Times, Georgia, serif;
	margin-bottom: 0.5em;
}

.profile-icon-desc p {
	margin-bottom: 0.5em;
	font-size: 12px !important;
	line-height:16px !important; /*addition*/
}

/* Profile List View */

.profile-list-row {
	display: inline-block;
	width: 740px;
	margin-bottom: 12px;
	padding-bottom: 12px;
	border-bottom: 1px solid #DCDCDC;
	clear: both;
}

.profile-list-row.last {
	margin-bottom: 0;
	padding-bottom: 0;
	border-bottom: 0;
}

.profile-list-row .profile-list-image {
	position: relative;
	display: block;
	float: left;
	margin-right: 12px;
}

.profile-list-row .profile-list-desc {
	position: relative;
	display: block;
}

.profile-list-row .headline {
	font: bold 18px/18px 'Times New Roman', Times, Georgia, serif;
	margin-bottom: 2px;
}

.profile-list-row .profile-list-desc p {
	font-size: 12px;
	margin-bottom: 0.5em; 
}

/* Profile Similar Sidebar */

