	position: relative;
	display: block;
	width: 200px;
	height: auto;
	margin-bottom: 20px;
	padding: 10px 8px 12px 8px;
	border: 1px solid #DCDCDC;
}

/*Drupal Additions*/
.region-sidebar-second {
	position: relative;
	display: block;
	width: 200px;
	height: auto;
	margin-bottom: 20px;
	padding: 10px 8px 12px 8px;
	/*border: 1px solid #DCDCDC;*/
}

.region-sidebar-second h2 { font: bold 18px/18px 'Times New Roman', Times, Georgia, serif; }






/*End Drupal Additions*/

.module-sidebar h3 { 
	margin-bottom: 10px;
}

.module-sidebar-desc, .module-sidebar-search-advanced { 
	margin-top: 10px;
}

.module-sidebar .module-sidebar-text {
	margin-bottom: 10px;
}

/* Sidebar Search Module */

.module-sidebar-search .module-sidebar-search-field {
	width: 150px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #808080;
	border: 1px solid #DCDCDC;
	padding: 5px;
}

.module-sidebar-search .module-sidebar-search-button {
	width: 30px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #FFFFFF;
	background: #000066;
	border: 1px solid #000066;
	padding: 5px;
	cursor: pointer;
}

/* Sidebar Submit Module */

.module-sidebar-submit .module-sidebar-submit-field {
	display: block;
	width: 185px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #808080;
	border: 1px solid #DCDCDC;
	margin-bottom: 5px;
	padding: 5px;
}

.module-sidebar-submit .module-sidebar-submit-button {
	width: 65px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #FFFFFF;
	background: #000066;
	border: 1px solid #000066;
	padding: 5px;
	cursor: pointer;
}

/* Sidebar List Module */

.module-sidebar .module-sidebar-list .module-sidebar-list-image {
	margin-bottom: 10px;
}

.module-sidebar .module-sidebar-list .module-sidebar-list-text {
	margin-bottom: 10px;
	padding-bottom: 10px;
	border-bottom: 1px solid #DCDCDC;
}

.module-sidebar .module-sidebar-list .module-sidebar-list-text .headline {
	margin-bottom: 10px;
	padding-bottom: 10px;
	border-bottom: 1px solid #DCDCDC;
	font-weight: normal; /*addition*/
	
}

.module-sidebar .module-sidebar-list .module-sidebar-list-text.last {
	margin-bottom: 0;
	padding-bottom: 0;
	border-bottom: 0;
}

/* Modules GLOBAL */ 

.module-sidebar .headline, .module-column-wide .headline {
	display: block;
}

.module-sidebar .date, .module-column-wide .date {
	color: #808080;
	font-size: 11px;
}

.module-sidebar .headline a, .module-column-wide .headline a {
	color: #000066 !important;
	border-bottom: 0 !important;
}

