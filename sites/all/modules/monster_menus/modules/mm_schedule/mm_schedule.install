/**
 * @file
 * Implementation of hook_install().
 */
function mm_schedule_install() {
  // Make sure this module loads after fieldgroup (which uses weight=9)
  db_update('system')
    ->fields(array('weight' => 10))
    ->condition('name', 'mm_schedule')
    ->execute();
}
